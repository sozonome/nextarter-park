{
  "name": "nextarter-park",
  "private": true,
  "scripts": {
    "dev": "rm -rf .next && next dev",
    "park-ui:add-component": "pnpm dlx @park-ui/cli add",
    "build": "next build",
    "build:turbo": "pnpm dlx turbo@latest build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "pnpm biome lint ./src",
    "biome:apply": "pnpm biome check --apply ./src",
    "type-check": "tsc --noEmit",
    "check:turbo": "pnpm dlx turbo@latest lint type-check",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "panda codegen && husky"
  },
  "dependencies": {
    "@ark-ui/react": "^2.2.3",
    "@park-ui/panda-preset": "^0.36.1",
    "@theme-toggles/react": "^4.1.0",
    "lucide-react": "^0.358.0",
    "next": "^14.1.3",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.5.0",
    "next-themes": "^0.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.1",
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@pandacss/dev": "^0.35.0",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.66",
    "husky": "^9.0.11",
    "knip": "^5.1.1",
    "lint-staged": "^15.2.2",
    "next-sitemap": "^4.2.3",
    "standard-version": "^9.5.0",
    "typescript": "5.4.2"
  },
  "engines": {
    "node": ">=16.8.x",
    "pnpm": ">=8"
  }
}
